{
  "semi": true,
  "singleQuote": true,
  "trailingComma": "none", // Remove trailing commas
  "tabWidth": 2,
  "printWidth": 80, // Adjust this to your preferred line length
  "jsxBracketSameLine": true, // Keep the closing bracket of JSX elements on the same line
  "arrowParens": "always", // Add parentheses to arrow function arguments always
  "insertPragma": false,
  "requirePragma": false,
  "proseWrap": "preserve",
  "endOfLine": "lf"
}
